fun loop_and_swap(arr: Array<number>, i: number, limit: number) {
    if (i < limit) {
        if (arr[i] > arr[i+1]) {
                arr.swap(i, i+1);
                sleep(4);
        }
    loop_and_swap(arr, i+1, limit);
    }
}

fun sort(arr: Array<number>, n: number) {
    if (n != 1) {
        loop_and_swap(arr,0, n -1);
        sort(arr, n-1);
    }
}

let array = Array<number>() { 1 + 4,6 ,9,8, 2};

sort(array, array.size());

